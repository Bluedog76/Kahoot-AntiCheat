{
    "manifest_version": 3,
    "name": "Kahoot Anti Cheat",
    "description": "Kahoot Anti Cheat made to stop people not play, inaportiate names, and more",
    "version": "0.1",
    "permissions": [
      "tabs",
      "storage",
      "activeTab"
    ],
    "action": {
      "default_popup": "scripts/popup/popup.html"
    },
    "icons": {
      "128": "images/128.png"
    },
    "content_scripts": [
      {
        "js": ["scripts/popup/content.js"],
        "matches": [
          "https://play.kahoot.it/*"
        ]
      }
    ]
  }